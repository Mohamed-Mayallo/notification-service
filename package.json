{
  "name": "notification-service",
  "version": "0.0.1",
  "description": "Notification Service",
  "author": "Mohamed Mayallo",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start --watch",
    "start:dev": "nest build --watch --webpack --webpackPath webpack-hmr.config.js",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "cross-env NODE_ENV=testing node --max-old-space-size=8192 --expose-gc ./node_modules/jest/bin/jest --detectOpenHandles --forceExit --logHeapUsage --verbose --runInBand",
    "test:watch": "cross-env NODE_ENV=testing node --max-old-space-size=8192 --expose-gc ./node_modules/jest/bin/jest --detectOpenHandles --forceExit --watchAll --logHeapUsage --verbose --runInBand",
    "test:cov": "cross-env NODE_ENV=testing node --max-old-space-size=8192 --expose-gc ./node_modules/jest/bin/jest --detectOpenHandles --forceExit --coverage --logHeapUsage --verbose --runInBand",
    "test:debug": "cross-env NODE_ENV=testing node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand"
  },
  "dependencies": {
    "@nestjs/bull": "^0.3.1",
    "@nestjs/common": "^7.6.13",
    "@nestjs/config": "^0.6.3",
    "@nestjs/core": "^7.6.13",
    "@nestjs/mongoose": "^7.2.4",
    "@nestjs/platform-express": "^7.6.13",
    "@nestjs/schedule": "^0.4.2",
    "@nestjs/serve-static": "^2.1.4",
    "bcryptjs": "^2.4.3",
    "bull": "^3.20.0",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "compression": "^1.7.4",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "express-rate-limit": "^5.1.3",
    "firebase-admin": "^9.2.0",
    "helmet": "^4.1.1",
    "ioredis": "^4.19.4",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.12.15",
    "nestjs-pino": "^1.2.0",
    "pino-multi-stream": "^5.1.1",
    "pino-pretty": "^4.2.1",
    "pino-sentry": "^0.6.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.3",
    "twilio": "^3.49.4",
    "uuid": "^8.3.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.5.6",
    "@nestjs/schematics": "^7.2.8",
    "@nestjs/testing": "^7.6.13",
    "@types/bcryptjs": "^2.4.2",
    "@types/bull": "^3.14.4",
    "@types/compression": "^1.7.0",
    "@types/express": "^4.17.8",
    "@types/express-rate-limit": "^5.1.0",
    "@types/faker": "^5.1.2",
    "@types/helmet": "^0.0.48",
    "@types/jest": "^26.0.14",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^14.11.2",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "4.3.0",
    "@typescript-eslint/parser": "4.3.0",
    "eslint": "7.10.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-import": "^2.22.1",
    "faker": "^5.1.0",
    "jest": "^26.4.2",
    "supertest": "^5.0.0",
    "ts-jest": "^26.5.0",
    "ts-loader": "^8.0.4",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.1.3",
    "typescript": "^4.0.3"
  }
}
